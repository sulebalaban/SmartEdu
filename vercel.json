{
    "version": 2,
    "builds": [
      { 
        "src": "smartedu/wsgi.py", 
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.13.1" 
        }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
      { "src": "/media/(.*)", "dest": "/media/$1" },
      { "src": "/(.*)", "dest": "smartedu/wsgi.py" }
    ]
  }
  