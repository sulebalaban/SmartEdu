{
    "version": 2,
    "builds": [
      { 
        "src": "smartedu/wsgi.py", 
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "smartedu/wsgi.py" 
      }
    ]
  }
  